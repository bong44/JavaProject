<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="350.0" prefWidth="520.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lecture17.tableview.TableViewController">
   <children>
      <TextField fx:id="nameField" layoutX="288.0" layoutY="14.0" promptText="이름" />
      <TextField fx:id="mobileNumberField" layoutX="288.0" layoutY="57.0" promptText="휴대전화번호" />
      <Button fx:id="addButton" disable="true" layoutX="288.0" layoutY="100.0" mnemonicParsing="false" onAction="#addAction" text="등록" />
      <Button layoutX="459.0" layoutY="100.0" mnemonicParsing="false" onAction="#deleteAction" text="삭제" />
      <TableView fx:id="membersTableView" layoutX="14.0" layoutY="14.0" prefHeight="324.0" prefWidth="261.0">
        <columns>
          <TableColumn fx:id="nameColumn" prefWidth="75.0" text="이름" />
          <TableColumn fx:id="mobileNumberColumn" prefWidth="185.0" text="휴대전화전호" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
