<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lecture17.imageexample.ImageController">
   <children>
      <Button fx:id="rightButton" layoutX="324.0" layoutY="137.0" mnemonicParsing="false" onAction="#changeImageAction" text="&gt;" />
      <ImageView fx:id="imageView" fitHeight="219.0" fitWidth="181.0" layoutX="114.0" layoutY="26.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@1.jpeg" />
         </image>
      </ImageView>
      <Button fx:id="leftButton" disable="true" layoutX="43.0" layoutY="137.0" mnemonicParsing="false" onAction="#backImageAction" text="&lt;" />
   </children>
</AnchorPane>
